<?xml version="1.0" encoding="UTF-8"?>
<clientRequest>
	<webServerVersion>1</webServerVersion>
	<date validateAs="unixDateTime"></date>
	<requestType>file_name</requestType>
	<options>
		<listLenght  validateAs="integer"></listLenght>
		<filterBy>
			<property> <!-- as host properties -->
				<operator validateAs="comparisonOperator"></operator>
				<value validateAs="operator"></value>
			</property>
		</filterBy>
		<orderBy>
			<property> <!-- as host properties -->
				<mode validateAs="orderingType"></mode>
			</property>
		</orderBy>
	</options>
</clientRequest>
